<sdf version="1.5">
    <world name="Example World">

        <model name="car">
            <pose> 5 3 0 0 0 0 </pose>
            <link name="link">
                <visual name="visual">
                    <geometry>
                        <box>
                            <size> 1 1 1 </size> # dependent on shape_type
                        </box>
                    </geometry>
                    <material>
                        <ambient> 1 0 0 1 </ambient>
                        <diffuse> 1 0 0 1 </diffuse>
                        <specular> 1 0 0 1 </specular>
                        <emissive> 1 0 0 1 </emissive>
                    </material>
                </visual>
            </link>
        </model>

        <model name="car2">
